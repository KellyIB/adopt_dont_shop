<h1><%= link_to "Shelter Index", "/shelters" %></h1>
<br></br>
<h2><%= link_to "Pets Index", "/pets" %></h2>
<br></br>

<%= form_tag '/shelters' do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>
<br></br>
  <%= label_tag :address %>
  <%= text_field_tag :address %>
<br></br>
  <%= label_tag :city %>
  <%= text_field_tag :city %>
<br></br>
  <%= label_tag :state %>
  <%= text_field_tag :state %>
<br></br>
  <%= label_tag :zip %>
  <%= text_field_tag :zip %>
<br></br>
  <%= submit_tag "Create Shelter" %>
  <% end %>
